{
    "$schema": "http://json-schema.org/schema",
    "id": "SchematicsNgAdd",
    "title": "Ng Add Schema",
    "type": "object",
    "properties": {
        "project": {
            "type": "string",
            "description": "The name of the angular project you want to add @ukis/core-ui.",
            "$default": {
                "$source": "defaultProject"
            }
