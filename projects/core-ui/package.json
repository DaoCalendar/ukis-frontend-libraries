{
  "name": "@dlr-eoc/core-ui",
  "version": "0.0.0-PLACEHOLDER",
  "main": "src/public-api",
  "license": "Apache-2.0",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "schematics:build": "npx ../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "schematics:copy:collection": "npx ../../node_modules/.bin/cpx schematics/collection.json ../../dist/core-ui/schematics/",
    "schematics:copy:schemas": "npx ../../node_modules/.bin/cpx 'schematics/**/schema.json' '../../dist/core-ui/schematics/'",
    "schematics:copy:files": "npx ../../node_modules/.bin/cpx 'schematics/*/files/**' '../../dist/core-ui/schematics/'",
    "schematics:copy:alert": "npx ../../node_modules/.bin/cpx 'src/lib/global-alert/**' '../../dist/core-ui/schematics/ng-add/files/src/app/components/global-alert/'",
    "schematics:copy:progress": "npx ../../node_modules/.bin/cpx 'src/lib/global-progress/**' '../../dist/core-ui/schematics/ng-add/files/src/app/components/global-progress/'",
    "postschematics:build": "npm run schematics:copy:collection && npm run schematics:copy:schemas && npm run schematics:copy:files && npm run schematics:copy:alert && npm run schematics:copy:progress",
    "preschematics:test": "npm run schematics:build",
    "schematics:test": "npx ../../node_modules/.bin/jasmine ../../dist/core-ui/schematics/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "@dlr-eoc",
    "core-ui"
  ],
  "ng-add": {
    "save": false
  },
  "dependencies": {
    "@angular-devkit/core": "0.0.0-PLACEHOLDER-VENDOR",
    "@angular-devkit/schematics": "0.0.0-PLACEHOLDER-VENDOR",
    "@schematics/angular": "0.0.0-PLACEHOLDER-VENDOR",
    "parse5-html-rewriting-stream": "^5.1.1"
  },
  "peerDependencies": {
    "@angular/common": "0.0.0-PLACEHOLDER-VENDOR",
    "@angular/core": "0.0.0-PLACEHOLDER-VENDOR",
    "@angular/router": "0.0.0-PLACEHOLDER-VENDOR",
    "@clr/angular": "0.0.0-PLACEHOLDER-VENDOR",
    "@clr/icons": "0.0.0-PLACEHOLDER-VENDOR",
    "rxjs": "0.0.0-PLACEHOLDER-VENDOR"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "typescript": "0.0.0-PLACEHOLDER-VENDOR",
    "jasmine": "^2.8.0",
    "zone.js": "0.0.0-PLACEHOLDER-VENDOR",
    "@angular/platform-browser-dynamic": "0.0.0-PLACEHOLDER-VENDOR"
  }
}
