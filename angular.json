    "demo-user-info": {
      "root": "projects/demo-user-info/",
      "sourceRoot": "projects/demo-user-info/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-user-info",
            "index": "projects/demo-user-info/src/index.html",
            "main": "projects/demo-user-info/src/main.ts",
            "polyfills": "projects/demo-user-info/src/polyfills.ts",
            "tsConfig": "projects/demo-user-info/tsconfig.app.json",
            "assets": [
              "projects/demo-user-info/src/favicon.ico",
              "projects/demo-user-info/src/assets"
            ],
            "styles": [
              "projects/demo-user-info/src/styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules"
              ]
            },
            "scripts": [
              "node_modules/@webcomponents/custom-elements/custom-elements.min.js",
              "node_modules/@clr/icons/clr-icons.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-user-info/src/environments/environment.ts",
                  "with": "projects/demo-user-info/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-user-info:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-user-info:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-user-info:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-user-info/src/test.ts",
            "polyfills": "projects/demo-user-info/src/polyfills.ts",
            "tsConfig": "projects/demo-user-info/tsconfig.spec.json",
            "karmaConfig": "projects/demo-user-info/karma.conf.js",
            "styles": [
              "projects/demo-user-info/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/demo-user-info/src/favicon.ico",
              "projects/demo-user-info/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo-user-info/tsconfig.app.json",
              "projects/demo-user-info/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
